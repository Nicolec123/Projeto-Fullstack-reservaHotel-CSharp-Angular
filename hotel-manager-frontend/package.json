{
  "name": "hotel-manager-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "start:dev": "set VITE_OPTIMIZE_DEPS=false && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
